{
  "inbounds": [
    {
      "type": "tuic",
      "tag": "tuic-in",
      "listen": "::",
      "listen_port": PORT,
      "auth_timeout": "3s",
      "congestion_control": "bbr",
      "heartbeat": "3s",
      "zero_rtt_handshake": false,
      "tls": {
        "alpn": [
          "h3"
        ],
        "enabled": true,
        "server_name": "www.apple.com",        
        "certificate_path": "/etc/tuic/server.crt",
        "key_path": "/etc/tuic/server.key"
      },
      "users": [
        {
          "name": "NAME",
          "password": "PASSWORD",
          "uuid": "UUID"
        }
      ]
    }
  ],
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "outbounds": [
    {
      "tag": "direct",
      "type": "direct"
    }
  ]
}
