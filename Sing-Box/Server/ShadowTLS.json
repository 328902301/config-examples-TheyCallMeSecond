{
  "dns": {
    "servers": [
      {
        "address": "tls://1.1.1.1"
      }
    ]
  },
  "inbounds": [
    {
      "type": "shadowtls",
      "detour": "shadowsocks-in",
      "listen": "::",
      "listen_port": PORT,
      "strict_mode": true,
      "version": 3,
      "handshake": {
        "server": "SNI",
        "server_port": 443
      },
      "users": [
        {
          "name": "NAME",
          "password": "STPASS"
        }
      ]
    },
    {
      "type": "shadowsocks",
      "tag": "shadowsocks-in",
      "listen": "127.0.0.1",
      "method": "chacha20-ietf-poly1305",
      "network": "tcp",
      "password": "SSPASS"
    }
  ],
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "outbounds": [
    {
      "tag": "direct",
      "type": "direct"
    }
  ]
}
