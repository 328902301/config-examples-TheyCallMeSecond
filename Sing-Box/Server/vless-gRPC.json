{
    "inbounds": [
        {
            "type": "vless",
            "tag": "vless-in",
            "listen": "::",
            "listen_port": PORT,
            "sniff": true,
            "sniff_override_destination": true,
            "tls": {
                "alpn": [
                    "http/1.1"
                ],
                "enabled": true,
                "server_name": "DOMAIN",
                "certificate_path": "/etc/grpc/server.crt",
                "key_path": "/etc/grpc/server.key"
            },
            "transport": {
                "type": "grpc",
                "service_name": "PATH",
                "idle_timeout": "15s",
                "ping_timeout": "15s",
                "permit_without_stream": false
            },
            "users": [
                {
                    "name": "NAME",
                    "uuid": "UUID"
                }
            ]
        }
    ],
    "log": {
        "level": "warn",
        "timestamp": true
    },
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct"
        }
    ]
}